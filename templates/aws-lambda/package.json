{
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt dev",
    "build": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt build",
    "build:watch": "NODE_PATH=$(pwd)/node_modules run --silent run --cwd ./node_modules/rajt build:watch",
    "export": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt export",
    "local": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt local",
    "zip": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt zip",
    "deploy": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt deploy"
  },
  "dependencies": {
    "rajt": "*"
  }
}