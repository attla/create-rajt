    "aws:build": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt aws:build",
    "cf:build": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt cf:build",
    "aws:build:watch": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt aws:build:watch",
    "cf:build:watch": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt cf:build:watch",
    "aws:export": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt aws:export",
    "cf:export": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt cf:export",
    "aws:package": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt aws:package",
    "aws:deploy": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt aws:deploy",
    "aws:update": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt aws:update",
