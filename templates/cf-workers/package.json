{
  "name": "rajt-cf-workers-template",
  "version": "0.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {

    "dev": "NODE_PATH=$(pwd)/node_modules bun run --silent --cwd ./node_modules/rajt dev",
