    "clean": "bun run --silent --cwd ./node_modules/rajt clean",
    "clean:build": "bun run --silent --cwd ./node_modules/rajt clean:build",
    "clean:temp": "bun run --silent --cwd ./node_modules/rajt clean:temp",
    "ensure-dirs": "bun run --silent --cwd ./node_modules/rajt ensure-dirs",
